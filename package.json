{
  "name": "pm2-discord-plus",
  "type": "module",
  "version": "0.1.3",
  "description": "A PM2 module to emit events to discord with style",
  "main": "index.js",
  "scripts": {
    "version": "standard-version --sign"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesNZL/pm2-discord-plus"
  },
  "keywords": [
    "pm2",
    "discord",
    "plus",
    "improved",
    "embed",
    "log"
  ],
  "contributors": [
    {
      "name": "James",
      "email": "james@jamesnzl.xyz"
    },
    {
      "name": "Naxey",
      "email": "max@naxey.de"
    },
    {
      "name": "Francisco",
      "email": "info@franciscog.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/JamesNZL/pm2-discord-plus/issues"
  },
  "homepage": "https://github.com/JamesNZL/pm2-discord-plus",
  "dependencies": {
    "dateformat": "^5.0.3",
    "pm2": "^4.1.2",
    "pmx": "^1.6.7",
    "request": "^2.88.0",
    "strip-ansi": "^6.0.0"
  },
  "config": {
    "discord_url": null,
    "process_name": null,
    "log": true,
    "error": false,
    "kill": true,
    "exception": true,
    "restart": false,
    "delete": false,
    "stop": true,
    "restart overlimit": true,
    "exit": false,
    "start": false,
    "online": false,
    "buffer": true,
    "buffer_seconds": 1,
    "queue_max": 100
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "standard-version": "^9.5.0"
  }
