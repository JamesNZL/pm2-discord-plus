{
  "name": "pm2-discord-plus",
  "version": "0.1.8",
  "description": "A PM2 module to emit events and logs to Discord, with style.",
  "main": "index.js",
  "scripts": {
    "version": "standard-version --sign",
    "publish": "git push --follow-tags origin master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesNZL/pm2-discord-plus"
  },
  "keywords": [
    "pm2",
    "discord",
    "plus",
    "improved",
    "embed",
    "log"
  ],
  "contributors": [
    {
      "name": "James",
      "email": "james@jamesnzl.xyz"
    },
    {
      "name": "Naxey",
      "email": "max@naxey.de"
    },
    {
      "name": "Francisco",
      "email": "info@franciscog.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/JamesNZL/pm2-discord-plus/issues"
  },
  "homepage": "https://github.com/JamesNZL/pm2-discord-plus",
  "dependencies": {
    "dateformat": "^4.6.3",
    "pm2": "^5.0.0",
    "pmx": "^1.6.7",
    "request": "^2.88.0",
    "strip-ansi": "^6.0.1"
  },
  "config": {
    "discord_url": null,
    "process_name": null,
    "embed_name": null,
    "embed_footer_icon": null,
    "date_format": "default",
    "log": true,
    "error": false,
    "kill": true,
    "exception": true,
    "restart": false,
    "delete": false,
    "stop": true,
    "restart overlimit": true,
    "exit": false,
    "start": false,
    "online": false,
    "buffer": true,
    "buffer_seconds": 1,
    "queue_max": 100
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "standard-version": "^9.5.0"
  }
